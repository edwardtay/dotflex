# Frontend Environment Variables
VITE_POLKADOT_RPC_URL=wss://rpc.polkadot.io
# VRF Lottery Contract Address (set after deployment)
VITE_VRF_LOTTERY_CONTRACT_ADDRESS=
# Moonbeam Alpha Contract Address (EVM)
VITE_MOONBEAM_CONTRACT_ADDRESS=0x1dc962e5461ba33f5021708d15a9c5945abfa508
# Subscan API Key (get from https://docs.subscan.io/)
# Example: VITE_SUBSCAN_API_KEY=your_api_key_here
VITE_SUBSCAN_API_KEY=

# QuickNode API (alternative to Subscan - more reliable)
# QuickNode endpoint for Polkadot mainnet
VITE_QUICKNODE_URL=
# QuickNode WSS endpoint (for RPC fallback if needed)
VITE_QUICKNODE_WSS_URL=
# API key if required (usually not needed for QuickNode endpoints)
VITE_QUICKNODE_API_KEY=

# Services Environment Variables
PORT=3001
POLKADOT_RPC_URL=wss://rpc.polkadot.io

